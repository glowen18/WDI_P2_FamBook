<h1>Welcome <%= current_user.name.capitalize %> to Your FamBook!</h1>

<table class="table table-striped table-bordered table-hover">
  <thead>
    <tr>
      <th>Family Member</th>
      <th>Email</th>
      <th>Mobile</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.mobile %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
<br>
